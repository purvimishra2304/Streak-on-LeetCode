bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
    int car[1001]={0};
    for(int i=0;i<tripsSize;i++){
        int start=trips[i][1];
        int end=trips[i][2];
        int pass=trips[i][0];
        car[start]=car[start]+pass;
        car[end]=car[end]-pass;
    }
    for(int i=1;i<1001;i++){
        car[i]=car[i]+car[i-1];
    }
    for(int i=0;i<1001;i++){
        if(car[i]>capacity){
            return false;
        }


    }
    return true;
}
